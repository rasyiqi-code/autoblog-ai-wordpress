{
    "name": "rasyiqi/autoblog",
    "description": "An intelligent autoblog plugin for WordPress that scrapes, processes, and publishes content.",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Rasyiqi",
            "email": "rasyiqi@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "phpoffice/phpword": "^1.2",
        "smalot/pdfparser": "^2.11",
        "fivefilters/readability.php": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Autoblog\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Autoblog\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true
    }
}
